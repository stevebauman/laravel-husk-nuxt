{
    "name": "stevebauman/laravel-husk",
    "description": "A Laravel Dusk environment for your Javascript only applications.",
    "license": "MIT",
    "authors": [
        {
            "name": "Steve Bauman",
            "email": "steven_bauman@outlook.com"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "laravel-zero/framework": "^9.0",
        "nunomaduro/laravel-console-dusk": "^1.10",
        "nunomaduro/termwind": "^1.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/dusk": "^6.22",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.1"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "archive": {
        "exclude": [
            "examples"
        ]
    },
    "minimum-stability": "dev"
}
